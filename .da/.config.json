{
  "version": 1,
  "org": "developer-arman",
  "repo": "aemdemo",
  "contentSourceUrl": "https://content.da.live/developer-arman/aemdemo/",
  "contentSourceType": "onedrive",
  "owner": "developer-arman",
  "site": "aemdemo",
  "host": "main--aemdemo--developer-arman.aem.live",
  "previewHost": "main--aemdemo--developer-arman.aem.page",
  "gitUrl": "https://github.com/developer-arman/aemdemo",
  "fstab": {
    "mountpoints": {
      "/": {
        "url": "https://content.da.live/developer-arman/aemdemo/",
        "type": "markup"
      }
    },
    "folders": {
      "/products/": "/products/default"
    }
  },
  "sidekick": {
    "project": "AEM Demo - developer-arman",
    "editUrlLabel": "Document Authoring",
    "editUrlPattern": "https://da.live/edit#/developer-arman/aemdemo{{pathname}}",
    "plugins": [
      {
        "id": "library",
        "title": "Block Library",
        "environments": ["edit"],
        "url": "/block-library.json",
        "isPalette": true
      }
    ]
  },
  "blocks": [
    {
      "name": "simple-card",
      "title": "Simple Card",
      "description": "A simple card block for displaying content",
      "path": "/blocks/simple-card/simple-card.js",
      "css": "/blocks/simple-card/simple-card.css"
    },
    {
      "name": "hero",
      "title": "Hero",
      "description": "A hero banner block for landing pages",
      "path": "/blocks/hero/hero.js",
      "css": "/blocks/hero/hero.css"
    },
    {
      "name": "highlight",
      "title": "Highlight",
      "description": "A block for highlighting important content",
      "path": "/blocks/highlight/highlight.js"
    }
  ]
}